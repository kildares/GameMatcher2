<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="silveira.avila.com.gamematcher2">

    <application
        android:name=".activity.GameMatcherApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/DefaultGameTheme">
        <activity android:name=".pick.PickTeamActivity" />
        <activity android:name=".player.PlayerDetailActivity" />
        <activity android:name=".match.MatchDetailActivity" />
        <activity android:name=".match.CreateMatchActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>